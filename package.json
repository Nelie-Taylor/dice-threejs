{
  "name": "@3d-dice/dice-box-threejs",
  "author": {
    "name": "Frank Ali"
  },
  "description": "A 3D environment for rolling game dice using threejs and cannon-es",
  "private": false,
  "keywords": [
    "3D",
    "dice",
    "roll",
    "roller",
    "javascript",
    "rpg",
    "dnd",
    "d&d",
    "tabletop"
  ],
  "version": "0.0.12",
  "license": "MIT",
  "homepage": "https://fantasticdice.games/",
  "repository": {
    "type": "git",
    "url": "https://github.com/3d-dice/dice-box-threejs"
  },
  "bugs": {
    "url": "https://github.com/3d-dice/dice-box-threejs/issues"
  },
  "dev-files": [
    "src"
  ],
  "dev-main": "./src/index",
  "file": [
    "dist",
    "public"
  ],
  "main": "./dist/dice-box-threejs.umd.js",
  "module": "./dist/dice-box-threejs.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dice-box-threejs.es.js",
      "require": "./dist/dice-box-threejs.umd.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "cannon-es": "^0.20.0",
    "just-debounce-it": "^3.2.0",
    "three": "^0.143.0"
  },
  "devDependencies": {
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@eslint/js": "^9.19.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.0.0",
    "@types/three": "^0.143.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-react": "^4.3.4",
    "cannon-es-debugger": "^1.0.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5.0.0",
    "vite": "^3.0.8",
    "vitest": "^3.0.5"
  },
  "type": "module"
}
